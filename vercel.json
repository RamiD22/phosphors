{
  "outputDirectory": "site",
  "cleanUrls": true,
  "rewrites": [],
  "redirects": [
    { "source": "/art/:slug-page.html", "destination": "/gallery/:slug", "permanent": true },
    { "source": "/tokenomics", "destination": "/token", "permanent": true },
    { "source": "/tokenomics.html", "destination": "/token", "permanent": true },
    { "source": "/rewards", "destination": "/token#rewards", "permanent": true },
    { "source": "/rewards.html", "destination": "/token#rewards", "permanent": true }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" }
      ]
    },
    {
      "source": "/art/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "ALLOWALL" }
      ]
    }
  ]
}
