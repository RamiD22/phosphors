# Phosphors Environment Variables
# Copy this file to .env and fill in the values

# =============================================================================
# SUPABASE - Database and Auth
# =============================================================================
SUPABASE_URL=                    # Your Supabase project URL (https://xxx.supabase.co)
SUPABASE_ANON_KEY=               # Public/anon key for client-side
SUPABASE_SERVICE_KEY=            # Service role key for server-side operations

# =============================================================================
# COINBASE DEVELOPER PLATFORM - Wallet Infrastructure
# =============================================================================
CDP_API_KEY_ID=                  # CDP API key ID
CDP_API_KEY_SECRET=              # CDP API key secret (the long private key)
NETWORK_ID=                      # Network to use (base-sepolia or base-mainnet)

# =============================================================================
# SMART CONTRACTS - Deployed Addresses
# =============================================================================
GENESIS_CONTRACT=                # PhosphorGenesis NFT contract address
PLATFORM_CONTRACT=               # Platform/marketplace contract address  
USDC_CONTRACT=                   # USDC token contract address on the network

# =============================================================================
# MINTER WALLET - Auto-minting approved submissions
# =============================================================================
MINTER_WALLET_ID=                # CDP wallet ID for the minter
MINTER_SEED=                     # Encrypted seed for the minter wallet (JSON)
MINTER_WALLET=                   # Minter wallet address (0x...) - receives payments

# =============================================================================
# FUNDER WALLET - Auto-funding new agent wallets
# =============================================================================
FUNDER_WALLET_ID=                # CDP wallet ID for the funder
FUNDER_SEED=                     # Encrypted seed for the funder wallet (JSON)
FUNDER_ETH_AMOUNT=0.01           # ETH amount to fund new wallets (default: 0.01)
FUNDER_USDC_AMOUNT=5             # USDC amount to fund new wallets (default: 5)

# =============================================================================
# TREASURY - Protocol fees and burns
# =============================================================================
TREASURY_WALLET=                 # Wallet to collect protocol fees (default: MINTER_WALLET)

# =============================================================================
# SESSION & SECURITY
# =============================================================================
SESSION_SECRET=                  # Secret for signing session cookies
ADMIN_SECRET=                    # Secret for admin API endpoints
ADMIN_API_KEYS=                  # Comma-separated list of admin API keys

# =============================================================================
# REPLICATE - AI Image Generation
# =============================================================================
REPLICATE_API_TOKEN=             # Replicate API token for image generation

# =============================================================================
# TWITTER/X API - Social Features (Optional)
# =============================================================================
X_API_KEY=                       # Twitter API key (v2)
X_API_SECRET=                    # Twitter API secret
X_ACCESS_TOKEN=                  # Twitter access token
X_ACCESS_SECRET=                 # Twitter access token secret
X_CLIENT_ID=                     # Twitter OAuth 2.0 client ID
X_CLIENT_SECRET=                 # Twitter OAuth 2.0 client secret
